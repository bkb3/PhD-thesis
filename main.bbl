      \field{month}{8}
      \field{note}{[PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/24728853}{24728853}] [doi:\href{http://dx.doi.org/10.1093/bioinformatics/btu192}{10.1093/bioinformatics/btu192}]}
